{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "visual-knight-webapp": {
      "root": "apps/visual-knight-webapp/",
      "sourceRoot": "apps/visual-knight-webapp/src",
      "projectType": "application",
      "prefix": "visual-knight",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/visual-knight-webapp",
            "index": "apps/visual-knight-webapp/src/index.html",
            "main": "apps/visual-knight-webapp/src/main.ts",
            "polyfills": "apps/visual-knight-webapp/src/polyfills.ts",
            "tsConfig": "apps/visual-knight-webapp/tsconfig.app.json",
            "assets": [
              "apps/visual-knight-webapp/src/favicon.ico",
              "apps/visual-knight-webapp/src/assets"
            ],
            "styles": ["apps/visual-knight-webapp/src/styles.scss"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/visual-knight-webapp/src/environments/environment.ts",
                  "with": "apps/visual-knight-webapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "visual-knight-webapp:build",
            "proxyConfig": "apps/visual-knight-webapp//proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "visual-knight-webapp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "visual-knight-webapp:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/visual-knight-webapp/tsconfig.app.json",
              "apps/visual-knight-webapp/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/visual-knight-webapp/jest.config.js",
            "tsConfig": "apps/visual-knight-webapp/tsconfig.spec.json",
            "setupFile": "apps/visual-knight-webapp/src/test-setup.ts"
          }
        }
      }
    },
    "visual-knight-webapp-e2e": {
      "root": "apps/visual-knight-webapp-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/builders:cypress",
          "options": {
            "cypressConfig": "apps/visual-knight-webapp-e2e/cypress.json",
            "tsConfig": "apps/visual-knight-webapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "visual-knight-webapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "visual-knight-webapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/visual-knight-webapp-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "backend-webapp": {
      "root": "apps/backend-webapp",
      "sourceRoot": "apps/backend-webapp/src",
      "projectType": "application",
      "prefix": "backend-webapp",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/builders:node-build",
          "options": {
            "outputPath": "dist/apps/backend-webapp",
            "main": "apps/backend-webapp/src/index.ts",
            "tsConfig": "apps/backend-webapp/tsconfig.app.json",
            "assets": ["apps/backend-webapp/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/backend-webapp/src/environments/environment.ts",
                  "with": "apps/backend-webapp/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/builders:node-execute",
          "options": {
            "buildTarget": "backend-webapp:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/backend-webapp/tsconfig.app.json",
              "apps/backend-webapp/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/backend-webapp/jest.config.js",
            "tsConfig": "apps/backend-webapp/tsconfig.spec.json"
          }
        }
      }
    },
    "backend-comparison": {
      "root": "apps/backend-comparison",
      "sourceRoot": "apps/backend-comparison/src",
      "projectType": "application",
      "prefix": "backend-comparison",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/builders:node-build",
          "options": {
            "outputPath": "dist/apps/backend-comparison",
            "main": "apps/backend-comparison/src/index.ts",
            "tsConfig": "apps/backend-comparison/tsconfig.app.json",
            "assets": ["apps/backend-comparison/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": false,
              "extractLicenses": true,
              "sourceMap": false,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/backend-comparison/src/environments/environment.ts",
                  "with": "apps/backend-comparison/src/environments/environment.prod.ts"
                }
              ],
              "webpackConfig": "api.webpack.config.js"
            }
          }
        },
        "serve": {
          "builder": "@nrwl/builders:node-execute",
          "options": {
            "buildTarget": "backend-comparison:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/backend-comparison/tsconfig.app.json",
              "apps/backend-comparison/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/backend-comparison/jest.config.js",
            "tsConfig": "apps/backend-comparison/tsconfig.spec.json"
          }
        }
      }
    },
    "prisma": {
      "root": "libs/prisma",
      "sourceRoot": "libs/prisma/src",
      "projectType": "library",
      "prefix": "platform-community-edition",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/prisma/tsconfig.lib.json",
              "libs/prisma/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/prisma/jest.config.js",
            "tsConfig": "libs/prisma/tsconfig.spec.json"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
    "prisma2": {
      "root": "libs/prisma2",
      "sourceRoot": "libs/prisma2/src",
      "projectType": "library",
      "prefix": "platform-community-edition",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/prisma2/tsconfig.lib.json",
              "libs/prisma2/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/prisma2/jest.config.js",
            "tsConfig": "libs/prisma2/tsconfig.spec.json"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "schematics": {
    "@nrwl/schematics:application": {
      "style": "scss"
    },
    "@nrwl/schematics:library": {
      "style": "scss"
    }
  },
  "defaultProject": "visual-knight-webapp"
}
